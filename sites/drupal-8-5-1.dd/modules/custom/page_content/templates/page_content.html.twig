{#
/**
 * @file page_content.html.twig
 * Default theme implementation to present Page content data.
 *
 * This template is used when viewing Page content pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_page_content()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('page_content') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
